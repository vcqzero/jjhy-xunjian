<?php 
use Zend\Paginator\Paginator;

$paginator  = $this->paginator;
$route      = $this->route;
$action     = $this->action;
$where      = $this->where;
?>
<?php if (count($paginator) && $paginator instanceof Paginator) :?>
	<div class="ln_solid"></div>
	<?= $this->paginationControl(
	    $paginator,
        'Sliding',
        'partial/admin/paginator/my_pagination_control.phtml',
	    ['where' => $where, 'route'=> $route, 'action'=>$action]
    ) ?>
<?php endif;?>